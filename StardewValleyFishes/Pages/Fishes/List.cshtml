@page
@model StardewValleyFishes.Pages.Fishes.ListModel
@{
    ViewData["Title"] = "List";
}

<h1>List</h1>

<table class="table">
    <thead>
        <tr>
            <th scope="col">Name</th>
            <th scope="col">Local</th>
            <th scope="col">Description</th>
            <th scope="col">Season</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var fish in Model.Fishes)
        {
        <tr>
            <td>
                @fish.Name
            </td>
            <td>@fish.Local</td>
            <td>@fish.Description</td>
            <td>@fish.Season</td>
        </tr>
        }
    </tbody>
</table>
